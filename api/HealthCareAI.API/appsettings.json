{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=db;Username=postgres;Password=pwd"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "healthcare-ai-development-secret-key-for-testing-32-chars-minimum-length-required",
    "Issuer": "HealthCareAI",
    "Audience": "HealthCareAIClient",
    "ExpirationInMinutes": 60,
    "RefreshTokenExpiryDays": 7
  },
  "OpenAI": {
    "ApiKey": "your_key"
  },
  "Security": {
    "AllowedOrigins": "http://localhost:3000,http://localhost:3001",
    "RequireHttps": false,
    "EnableRateLimiting": false
  },
  "Caching": {
    "Redis": {
      "ConnectionString": "localhost:6379",
      "InstanceName": "HealthCareAI_Dev"
    },
    "DefaultExpirationMinutes": 30,
    "UserPermissionExpirationMinutes": 15
  }
}
